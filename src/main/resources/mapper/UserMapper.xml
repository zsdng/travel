<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.fy.dao.UserDao">

    <select id="findByUsername" resultType="User">
        select * from user where username=#{username}
    </select>

    <insert id="addUser" parameterType="cn.fy.domain.User">

        insert into user(username,password,name,birthday,sex,telephone,email)
        values (#{username}, #{password},#{name},#{birthday},#{sex},#{telephone},#{email})

    </insert>







</mapper>